          />\n            </router-link>\n          <router-link\n            :to=\"{ name: 'view', params: { id: post.id, slug: post.slug } }\"\n            >{{ post.name }}\n          </router-link>\n          <hr />\n        </div>\n      </div>\n      <div class=\"d-flex bg-dark justify-content-center\">\n        <pagination\n          v-model=\"page\"\n          :records=\"total\"\n          :per-page=\"perPage\"\n          @paginate=\"loadCategory\"\n          :options=\"{ texts: { count: '' } }\"\n        />\n      </div>\n    </div>\n    <Footer />\n  </div>\n</template>\n<script>\nimport backer from \"../../utils/axios\";\n\nimport Header from \"../../components/Header\";\nimport Footer from \"../../components/Footer\";\n\nimport Pagination from \"vue-pagination-2\";\n\nexport default {\n  data() {\n    return {\n      category: \"\",\n      posts: [],\n      total: 0,\n      perPage: 10,\n      page: 1,\n    };\n  },\n  methods: {\n    loadCategory: function (page = 1) {\n      let id = this.$route.params.id;\n      backer.get(`category/${id}?page=${page}`).then((response) => {\n        this.category = response.data.category;\n        this.posts = response.data.posts;\n        this.total = response.data.posts.length;\n      });\n    },\n  },\n  components: {\n    Header,\n    Pagination,\n    Footer,\n  },\n  mounted() {\n    this.loadCategory();\n  },\n};\n</script>\n<style scoped>\n</style>\n"]}]}